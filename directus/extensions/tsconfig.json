{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "removeComments": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true
  },
  "exclude": ["./src/modules"]
}
